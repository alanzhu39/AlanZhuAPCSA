public class Lab14d
{
	public static void main( String args[] )
	{
		//add test cases
		double[] a = {100,90,85,72.5,95.6};
		Grades test = new Grades();
		test.setGrades(5, a);
		System.out.println(test);
		double[] b = {50.0,100.0,80.0};
		test.setGrades(3, b);
		System.out.println(test);
		double[] c = {93.4,-90.0,90.0};
		test.setGrades(3, c);
		System.out.println(test);
		double[] d = {1,2,3,4,5,6,7,8,9};
		test.setGrades(9, d);
		System.out.println(test);
		
	}
}